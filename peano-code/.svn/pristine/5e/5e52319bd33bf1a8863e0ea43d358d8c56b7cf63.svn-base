body,table,td,tr,div,p,h1,h2,h3,h4,ul,li,a,center {
  font-family:Verdana,sans-serif;
  text-decoration:none;
  font-size:10pt;
  color:#000000;
  background-color:#FFffFF;
}


a:hover {
  text-decoration:underline;
}

a {
  text-decoration:none;
  font-weight:bold;
  font-size:10pt;
  border-style:none;
}

img {
  border:0pt;
}


table {
  border:0pt;
}



td.menu {
  margin-top:5cm;
  margin-right:5cm;
  margin-left:5cm;
  vertical-align:top;
  align:right;
  width:20%;
}


td.content {
  margin-top:5cm;
  margin-right:5cm;
  margin-left:5cm;
  vertical-align:top;
  width:80%;
}


p {
  text-decoration:none;
  margin-right:5cm;
  margin-left:2.5cm;
  text-align:left;
}

h1 {
  font-size:20pt;
  letter-spacing:3px;
  font-weight:bold;
  margin-top:100px;
  margin-left:5.5cm;
}

h2 {
  font-size:14pt;
  font-weight:bold;
  margin-top:100px;
  margin-left:5.5cm;
}
